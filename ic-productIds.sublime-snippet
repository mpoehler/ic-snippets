  <content><![CDATA[
(function() {
  if (BrightTag.Types.isArray(pageData.products) && pageData.products.length > 0) {
    var prodArr = [];
    for (var i = 0; i < pageData.products.length; i++) {
      prodArr.push(pageData.products[i].id);
    }
    return prodArr;
  }
  return "";
})();
