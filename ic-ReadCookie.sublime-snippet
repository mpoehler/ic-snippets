/* Version 1.0 Tini */
function readCookie(a) {
 for (var b = a + "=", c = document.cookie.split(";"), d = 0; d < c.length; d++) {
   for (var e = c[d];
     " " == e.charAt(0);)
     e = e.substring(1, e.length);
   if (0 == e.indexOf(b))
     return unescape(e.substring(b.length, e.length))
 }
 return null
}
]]></content>
